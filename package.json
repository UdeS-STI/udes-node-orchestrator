{
  "name": "udes-node-orchestrator",
  "version": "2.3.0",
  "description": "Orchestrator server. Handles routes and HTTP traffic.",
  "author": "Universit√© de Sherbrooke",
  "main": "dist/index.js",
  "scripts": {
    "audit": "node_modules/.bin/nsp check",
    "build": "babel ./src --out-dir dist --ignore '**/*Test.js'",
    "coverage": "NODE_ENV=test node_modules/.bin/nyc --require babel-core/register -x '**/config/**' -x '**/**Test.js' -x '**/dependencies/**' node_modules/.bin/mocha --opts ../mocha.opts \\ ./src/**/**Test.js",
    "documentation": "documentation build src/index.js -f html -o docs",
    "format": "eslint ./src --fix",
    "lint": "eslint ./src",
    "start": "babel-node boilerplate/src/index.js | pino",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --opts ../mocha.opts ./src/**/**Test.js"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "boom": "^7.1.1",
    "compression": "^1.7.1",
    "connect-couchbase": "^0.2.10",
    "cookie-parser": "^1.4.3",
    "couchbase": "2.3.0",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-pino-logger": "^2.0.0",
    "express-session": "^1.15.6",
    "lodash": "^4.17.4",
    "method-override": "^2.3.10",
    "pino": "^4.8.2",
    "request": "^2.83.0",
    "udes-connect-cas": "^1.0.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "chai-integer": "^0.1.0",
    "documentation": "^5.3.2",
    "eslint-config-udes": "^0.1.6",
    "mocha": "^4.0.1",
    "nsp": "^3.1.0",
    "nyc": "^11.2.1",
    "sinon": "^4.0.2",
    "sinon-chai": "^3.0.0"
  },
  "greenkeeper": {
    "ignore": [
      "couchbase"
    ]
  }
