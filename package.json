{
  "name": "udes-node-orchestrator",
  "version": "0.1.2",
  "description": "Orchestrator server. Handles routes and HTTP traffic.",
  "main": "distribution/index.js",
  "scripts": {
    "build": "babel ./src --out-dir distribution --ignore '**/*Test.js'",
    "coverage": "NODE_ENV=test node_modules/.bin/nyc --require babel-core/register -x '**/config/**' -x '**/**Test.js' -x '**/dependencies/**' node_modules/.bin/mocha --opts ../mocha.opts \\ ./src/**/**Test.js",
    "documentation": "documentation build src/index.js -f html -o docs",
    "lint": "eslint ./src",
    "start": "babel-node boilerplate/src/index.js | pino",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --opts ../mocha.opts ./src/**/**Test.js",
    "audit": "node_modules/.bin/nsp check"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.18.2",
    "boom": "5.2.0",
    "compression": "1.7.1",
    "connect-couchbase": "0.2.10",
    "cookie-parser": "1.4.3",
    "couchbase": "2.3.0",
    "debug": "3.1.0",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "express-pino-logger": "2.0.0",
    "express-session": "1.15.6",
    "lodash": "4.17.4",
    "method-override": "2.3.10",
    "moment": "2.18.1",
    "pino": "4.7.2",
    "request": "2.82.0",
    "udes-connect-cas": "1.0.2",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.1.2",
    "chai-http": "3.0.0",
    "chai-integer": "0.1.0",
    "documentation": "^5.3.2",
    "eslint-config-udes": "^0.1.5",
    "mocha": "3.5.3",
    "nsp": "2.8.1",
    "nyc": "11.2.1",
    "sinon": "3.3.0",
    "sinon-chai": "2.13.0"
  }
}
